                <button type="button" class="btn btn-default" ng-click="removeEducationDegreeModal()"><span hm-translate="cancel"></span></button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->

